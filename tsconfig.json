{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "moduleResolution": "node",
        "checkJs": true,
        "noEmit": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*.js"],
    "exclude": ["node_modules", "**/*.spec.js"]
}
