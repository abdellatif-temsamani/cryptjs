    "compilerOptions": {
        "target": "es2022",
        "module": "es2022",
        "moduleResolution": "nodenext",
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
